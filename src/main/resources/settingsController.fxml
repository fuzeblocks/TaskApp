<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="fr.fuzeblocks.taskapp.viewers.controllers.SettingsController"
            prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #333;">
    <Label text="Language:" textFill="WHITE" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <font>
            <Font name="System Bold" size="19.0" />
        </font></Label>
    <ChoiceBox fx:id="languageBox" onMouseClicked="#languageBox" prefHeight="25.0" prefWidth="400.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
</AnchorPane>
